        },
        {
            "style": "Text",
            "text": "In summary:",
            "updated": 1615839074979,
            "translations": [
                {
                    "language": "IT",
                    "text": "In sintesi:",
                    "updated": 1619545801000,
                    "style": "Text"
                },
