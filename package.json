{
  "name": "superalgos",
  "version": "1.3.0",
  "description": "Superalgos is a free, open-source crypto trading bot",
  "license": "Apache License 2.0",
  "homepage": "https://www.superalgos.org/",
  "author": {
    "name": "Superalgos Contributors"
  },
  "scripts": {
    "start": "node platform minMemo noBrowser",
    "presetup": "git checkout develop && git pull upstream develop",
    "setup": "node setup noShortcuts",
    "setupWithTf": "node setupWithTf noShortcuts",
    "postsetup": "npm start",
    "dist": "electron-builder -c build/electron-builder.json",
    "postinstall": "electron-builder install-app-deps",
    "startNetwork": "node network",
    "startSocialTrading": "node socialTrading",
    "installSocialTradingReact": "cd ./Social-Trading/React-UI && npm ci",
    "startLocalSocialTradingReact": "npm run startNetwork && cd ./Social-Trading/React-UI && npm run startSocialTradingReactBackend && npm run startSocialTradingReactFrontend",
    "startSocialTradingReact": "cd ./Social-Trading/React-UI && npm run startSocialTradingReactBackend && npm run startSocialTradingReactFrontend",
    "unitTest": "jest --silent",
    "unitTest:coverage": "jest --silent --coverage",
    "unitTest:debug": "DEBUG=nock.* jest",
    "lintAll": "eslint . --ext .js",
    "lintFile": "eslint --ext .js",
    "prepare": "husky install",
    "socialTradingAppDev": "cd ./Social-Trading/Vue-UI && vite dev",
    "socialTradingAppBuild": "cd ./Social-Trading/Vue-UI && vite build",
    "socialTradingAppPreview": "cd ./Social-Trading/Vue-UI && vite preview",
    "build": "webpack build --config ./Dashboards/webpack.config.js",
    "serve": "webpack-dev-server --mode development --config ./Dashboards/webpack.config.js"
  },
  "dependencies": {
    "@discordjs/rest": "^1.4.0",
    "@octokit/rest": "^19.0.5",
    "@reduxjs/toolkit": "^1.9.1",
    "@slack/web-api": "^6.8.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "axios": "^1.2.1",
    "bootstrap": "*",
    "bootstrap-vue": "*",
    "ccxt": "^2.2.79",
    "child_process": "^1.0.2",
    "discord-api-types": "^0.37.21",
    "discord.js": "^14.7.1",
    "dotenv": "16.0.3",
    "dotenv-expand": "9.0.0",
    "electron-updater": "^5.3.0",
    "ethereumjs-common": "^1.5.2",
    "ethereumjs-tx": "^2.1.2",
    "ethers": "^5.7.2",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "hyperquest": "^2.1.3",
    "ip": "^1.1.8",
    "jsdom": "^20.0.3",
    "lookpath": "^1.2.2",
    "ndjson": "^2.0.0",
    "node-fetch": "^3.3.0",
    "npm": "^9.1.3",
    "open": "^8.4.0",
    "path": "^0.12.7",
    "pm2": "^5.2.2",
    "simple-git": "^3.15.1",
    "telegraf": "^4.11.2",
    "twitter-api-v2": "^1.12.9",
    "util": "^0.12.5",
    "vader-sentiment": "^1.1.3",
    "vite": "^3.2.5",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "web3": "^1.8.1",
    "workbox-webpack-plugin": "^6.5.4",
    "ws": "^8.11.0",
    "yargs": "^17.6.2"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs-node": "^4.1.0"
  },
  "devDependencies": {
    "css-loader": "^6.7.2",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.29.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "nock": "^13.2.9",
    "vue-loader": "^17.0.1",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "transform": {
      "^.+\\.[m|t|j]s?$": "babel-jest"
    },
    "testMatch": [
      "**/.tests/**/?(*.)(test).js?(x)",
      "**/.tests/?(*.)(test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "coverageDirectory": "<rootDir>/.testsCoverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/Launch-Scripts/*.ico",
      "<rootDir>/Launch-Scripts/*.sh"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  }
}
